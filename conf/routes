GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET	    /							controllers.ZettelController.index()
GET 	/forms						controllers.ZettelController.getForms(id : String?=null,format:String?=null,documentId:String?=null)
POST	/forms  				    controllers.ZettelController.postForm(id : String?=null,format:String?=null,documentId:String?=null)
GET	    /client						controllers.ZettelController.client(format : String?="json",documentId:String?=null)